<div class="row collapse module-list-index <%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%= team_member_team_presenter.id %>">

    <div class="large-1 hide-for-medium-down columns" data-equalizer-watch="module-list">
      <span class="drag-helper"></span>
    </div>



    <div class="large-8 small-12 columns" data-equalizer-watch="module-list">

    <%= team_member_team_presenter.name %>
  </div>



  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= team_member_team_presenter.toggle_link %>
  </div>


  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= team_member_team_presenter.edit_link %>
  </div>

  <div class="large-1 hide-for-medium-down columns last" data-equalizer-watch="module-list">
    <%= team_member_team_presenter.delete_link %>
  </div>
</div>
