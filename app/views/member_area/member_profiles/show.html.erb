<% present @dashboard.member_profile do |member_profile_presenter| %>
  <% if member_profile_presenter.ready_to_submit? %>
    <%= link_to "Submit your data", new_member_area_member_submission_path %>
  <% elsif member_profile_presenter.member_submission? %>
    <%= link_to "Registration Data", member_area_registration_data_path %>
  <% end %>
<% end %>

<%= render partial: "member_area/team_members/team_member", object: @dashboard.team_member if @dashboard.team_member? %>

<%= render(@dashboard.admin_messages) if @dashboard.admin_messages? %>